#윤년

#1트
Year = int(input())
if Year % 4 == 0:
  if Year % 100 != 0:
    if Year % 400 == 0:
        print('1')
else:
    print('0')


#2트
Year = int(input())
if Year % 4 == 0:
    if Year % 100 != 0:
        print('1')
    elif Year % 400 == 0:
        print('1')
else:
    print('0')


#3트
Year = int(input())
if (Year % 4 == 0 and Year % 100 != 0) or Year % 400 == 0:
    print('1')
else: print('0')
